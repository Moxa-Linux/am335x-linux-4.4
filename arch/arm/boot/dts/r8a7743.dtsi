					R8A7743_CLK_VCP0 R8A7743_CLK_VPC0
					R8A7743_CLK_TMU1 R8A7743_CLK_3DG
					R8A7743_CLK_2DDMAC R8A7743_CLK_FDP1_1
					R8A7743_CLK_FDP1_0 R8A7743_CLK_TMU3
