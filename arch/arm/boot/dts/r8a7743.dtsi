				clocks = <&zs_clk>, <&zs_clk>, <&p_clk>,
					 <&zg_clk>, <&zs_clk>, <&zs_clk>,
					 <&zs_clk>, <&p_clk>, <&p_clk>,
					 <&rclk_clk>, <&cp_clk>, <&zs_clk>,
					 <&zs_clk>, <&zs_clk>;
