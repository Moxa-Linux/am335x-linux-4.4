/dts-v1/;

#include "am33xx.dtsi"
#include <dt-bindings/pwm/pwm.h>
#include <dt-bindings/interrupt-controller/irq.h>

/ {
	model = "MOXA UC-8100";
	compatible = "ti,moxa-uc8100", "ti,am33xx";

	cpus {
		cpu@0 {
			cpu0-supply = <&vdd1_reg>;
		};
	};

	cpu0_opp_table: opp_table0 {

		opp50@300000000 {
			status = "okay";
		};

		opp100@275000000 {
			status = "disabled";
		};

		opp100@300000000 {
			status = "disabled";
		};

		opp100@500000000 {
			status = "disabled";
		};

		opp100@600000000 {
			status = "okay";
		};

		opp120@600000000 {
			status = "disabled";
		};

		opp120@720000000 {
			status = "okay";
		};

		oppturbo@720000000 {
			status = "disabled";
		};

		oppturbo@800000000 {
			status = "disabled";
		};

		oppnitro@1000000000 {
			status = "okay";
		};

	};

	memory {
		device_type = "memory";
		reg = <0x80000000 0x20000000>; /* 512 MB */
	};

	vbat: fixedregulator@0 {
		compatible = "regulator-fixed";
	};

	/* Power supply provides a fixed 3.3V @3A */
	vmmcsd_fixed: fixedregulator@1 {
	      compatible = "regulator-fixed";
	      regulator-name = "vmmcsd_fixed";
	      regulator-min-microvolt = <3300000>;
	      regulator-max-microvolt = <3300000>;
	      regulator-boot-on;
	};

	wlan_en_regulator: fixedregulator@2 {
		compatible = "regulator-fixed";
		regulator-name = "wlan_en_regulator";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		startup-delay-us = <70000>;

		/* WLAN_EN GPIO for this board */
		gpios = <&gpio2 22 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	};

	leds {
		//pinctrl-0 = <&user_leds_s0>;
		compatible = "gpio-leds";

		led@1 {
			label = "UC8100:GREEN:CEL3";
			gpios = <&gpio1 4 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		led@2 {
			label = "UC8100:YELLOW:CEL2";
			gpios = <&gpio1 5 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		led@3 {
			label = "UC8100:RED:CEL1";
			gpios = <&gpio1 6 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		led@4 {
			label = "UC8100:GREEN:DIA3";
			gpios = <&gpio1 12 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		led@5 {
			label = "UC8100:YELLOW:DIA2";
			gpios = <&gpio1 13 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		led@6 {
			label = "UC8100:RED:DIA1";
			gpios = <&gpio1 14 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		led@7 {
			label = "UC8100:GREEN:ZIG";
			gpios = <&gpio2 2 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		led@8 {
			label = "UC8100:GREEN:SD";
			gpios = <&gpio2 3 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		led@9 {
			label = "UC8100:GREEN:USB";
			gpios = <&gpio2 4 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
	};

	buttons: push_button {
		compatible = "gpio-keys";
	};

};

